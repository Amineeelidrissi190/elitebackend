{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "command": "composer install"  // Assurez-vous que composer install est inclus dans la phase de build si n√©cessaire
  },
  "deploy": {
    "runtime": "V2",
    "numReplicas": 1,
    "startCommand": "php artisan config:cache && php artisan migrate && php artisan db:seed && php artisan serve --host 0.0.0.0 --port 10000",
    "sleepApplication": false,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
